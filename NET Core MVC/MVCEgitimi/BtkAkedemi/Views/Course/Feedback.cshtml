@model Candidate

<div>
    <h3 class="display-4">Tebrikler @Model.FullName</h3>
    <p class="lead">başvuru <mark>kabul edildi</mark></p>
    <p class="lead">ilgi için saol git şimdi</p>
    <hr />
    <blockquote class="blockquote">
        başvurunu @Model.SelectedCourse üzerine yaptınız.
    </blockquote>
    <p class="text-muted">başvuru tarihi @Model.ApplyAt</p>
    <div>
        <a class="btn btn-outline-success" asp-action="Index">Aplications</a>
        <a class="btn btn-success" asp-controller="Home" asp-action="Index"> Home</a>
    </div>
</div>